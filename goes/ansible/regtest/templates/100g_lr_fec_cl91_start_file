hset -q platina vnet.eth-31-1.speed 100g

hset -q platina vnet.eth-32-1.media copper
hset -q platina vnet.eth-32-1.speed 100g

hset -q platina vnet.pollInterval 1

! ifdown -a --allow vnet
! ifup -a --allow vnet

